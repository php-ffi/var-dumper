{
    "name": "ffi/var-dumper",
    "type": "library",
    "description": "List of symfony/var-dumper casters with FFI support",
    "license": "MIT",
    "keywords": ["ffi", "var-dumper", "symfony", "casters"],
    "support": {
        "source": "https://github.com/php-ffi/var-dumper",
        "issues": "https://github.com/php-ffi/var-dumper/issues",
        "docs": "https://github.com/php-ffi/var-dumper/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru",
            "homepage": "https://serafimarts.ru",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "FFI\\VarDumper\\": "src"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "require-dev": {
        "vimeo/psalm": "^5.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\VarDumper\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
